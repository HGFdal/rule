name: 网易云VIP音乐试听
desc: 解锁VIP音乐试听 by伟人
# 源:https://raw.githubusercontent.com/WeiRen0/Scripts/main/wyun.js

http:
  mitm:
    - "*.music.163.com"
  script:
    - match: ^http[s]?:\/\/.+music.+(player\/url|playlist|entrance|\/eapi\/search\/).*$
      name: "wyun"
      type: request
      timeout: 30
      require-body: false
      max-size: 0
      binary-mode: false

script-providers:
  "wyun":
    url: https://raw.githubusercontent.com/WeiRen0/Scripts/main/wyun.js
    interval: 86400
